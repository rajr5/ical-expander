{
  "compilerOptions": {
    "target": "es3",
    "module": "commonjs",
    "outDir": "./dist",
    "lib": [
      "ES2015"
    ],
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "typeRoots": ["./types"],
  },
  "include": [
    "src"
  ],
}
